{
  "name": "steveproxy",
  "version": "1.0.0",
  "description": "Minecraft Proxy for sending custom events without modding",
  "main": "./src/index.ts",
  "type": "module",
  "scripts": {
    "start": "node --experimental-modules --experimental-json-modules --unhandled-rejections=warn --es-module-specifier-resolution=node ./dist/index.js",
    "build": "tsc && node --experimental-modules --experimental-json-modules --es-module-specifier-resolution=node ./scripts/cleanUpBuild.js",
    "prebuild": "node --experimental-modules --experimental-json-modules --es-module-specifier-resolution=node ./scripts/buildInit.js",
    "eslint:check": "eslint ./src/**/*",
    "eslint:fix": "eslint ./src/**/* --fix"
  },
  "author": "MrZillaGold <Pronin Egor | mrzillagold@gmail.com>",
  "license": "CC-BY-NC-ND-4.0",
  "dependencies": {
    "axios": "^0.21.1",
    "js-string-compression": "^1.0.1",
    "lowdb": "^1.0.0",
    "minecraft-data": "^2.77.0",
    "minecraft-path": "^1.0.0",
    "minecraft-protocol": "^1.23.2",
    "rawjsonbuilder": "^1.1.2",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@types/lowdb": "^1.0.9",
    "@types/node": "^14.14.31",
    "@types/spotify-web-api-node": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "typescript": "^4.2.2"
  }
}
